<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fat"
    android:background="@drawable/mental">

    <ImageView
        android:id="@+id/icon"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/icon" />

    <Button
        android:id="@+id/back"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="300dp"
        android:background="@drawable/rounded_corners"
        android:text="عودة"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/textView16"
        android:autoLink="all"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="قليل الدسم ١: https://www.webmd.com/women/reducing-dietary-fat#1 "
        android:layout_marginTop="150dp"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/textView17"
        android:autoLink="all"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="قليل الدسم ٢: https://www.medicalnewstoday.com/articles/325860 "
        android:layout_marginTop="220dp"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/textView18"
        android:autoLink="all"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="قليل الدسم ٣: https://www.drugs.com/cg/low-fat-diet.html"
        android:layout_marginTop="290dp"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/textView19"
        android:autoLink="all"
        android:clickable="true"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="قليل الدسم ٤: https://patient.info/news-and-features/low-fat-diet-sheet "
        android:layout_marginTop="360dp"
        android:textSize="20sp"/>

</RelativeLayout>